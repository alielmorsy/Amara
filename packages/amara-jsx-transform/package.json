{
  "name": "@amara-jsx-transform",
  "version": "1.0.0",
  "description": "JSX transformation plugin for Amara",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "amara-jsx": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./babel": {
      "import": "./dist/babel.mjs",
      "require": "./dist/babel.js",
      "types": "./dist/babel.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rimraf dist .tsbuildinfo"
  },
  "dependencies": {
    "jsx": "^0.9.89"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-react-jsx": "latest",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.14.1",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "@amara/js": "workspace:*",
    "tsup": "^8.0.1",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "@amara/js": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Ali Elmorsy <ali.elmorsy19@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jsx",
    "babel",
    "react",
    "compiler",
    "plugin",
    "amara"
  ]
}
